{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "title": {
    "text": "Power Station Installations by Capacity and Average Output",
    "fontSize": 20,
    "anchor": "middle",
    "font": "Helvetica Neue, Arial",
    "color": "#00382f"
  },
  "width": 1000,
  "height": 500,
  "background": "#FFFDF7",
  "data": {
    "url": "https://angus135.github.io/data_viz2_FIT3179/data/project_time_data.json"
  },
  "transform": [
    {
      "calculate": "datum.State == 'VIC' ? 'Victoria' : datum.State == 'NSW' ? 'New South Wales' : datum.State == 'QLD' ? 'Queensland' : datum.State == 'SA' ? 'South Australia' : datum.State == 'NT' ? 'Northern Territory' : datum.State == 'WA' ? 'Western Australia' : datum.State == 'ACT' ? 'Australian Capital Territory' : datum.State == 'TAS' ? 'Tasmania' : datum.State",
      "as": "State Full Name"
    }
  ],
  "layer": [
    {
      "mark": {
        "type": "circle",
        "opacity": 0.7
      },
      "encoding": {
        "x": {
          "field": "Accreditation_Date_ISO",
          "type": "temporal",
          "title": "Accreditation Date",
          "scale": {
            "domain": ["2000-01-01", "2020-01-01"]
          },
          "axis": {
            "format": "%Y",
            "labelAngle": 0,
            "titleFont": "Helvetica Neue, Arial",
            "titleFontSize": 13,
            "labelFont": "Lato, sans-serif",
            "labelFontSize": 11,
            "grid": true,
            "gridOpacity": 0.5,
            "titleColor": "#00382f",
            "labelColor": "#00382f"
          }
        },
        "y": {
          "field": "Installed_Capacity_MW",
          "type": "quantitative",
          "title": "Log Scaled Installed Capacity (MW)",
          "scale": {
            "type": "log"
          },
          "axis": {
            "titleFont": "Helvetica Neue, Arial",
            "titleFontSize": 13,
            "labelFont": "Lato, sans-serif",
            "labelFontSize": 11,
            "grid": true,
            "gridOpacity": 0.5,
            "titleColor": "#00382f",
            "labelColor": "#00382f"
          }
        },
        "size": {
          "field": "Avg_MW",
          "type": "quantitative",
          "title": "Average Output (MW)",
          "scale": {
            "range": [10, 2000]
          },
          "legend": {
            "titleFont": "Helvetica Neue, Arial",
            "titleFontSize": 13,
            "labelFont": "Lato, sans-serif",
            "labelFontSize": 11,
            "orient": "none",
            "direction": "horizontal",
            "values": [0, 50, 100, 200],
            "legendX": 755,
            "legendY": 10,
            "fillColor": "#FFFDF7",
            "strokeColor": "gray",
            "padding": 10,
            "cornerRadius": 3,
            "titleColor": "#00382f",
            "labelColor": "#00382f"
          }
        },
        "color": {
          "field": "Fuel_Source",
          "type": "nominal",
          "title": "Fuel Source",
          "scale": {
            "domain": ["Biomass", "Hydro", "Wind", "Solar"],
            "range": ["#8B4513", "#08306b", "#78909C", "#FFA726"]
          },
          "legend": {
            "titleFont": "Helvetica Neue, Arial",
            "titleFontSize": 13,
            "labelFont": "Lato, sans-serif",
            "labelFontSize": 11,
            "orient": "none",
            "direction": "horizontal",
            "legendX": 505,
            "legendY": 10,
            "fillColor": "#FFFDF7",
            "strokeColor": "gray",
            "padding": 10,
            "cornerRadius": 3,
            "titleColor": "#00382f",
            "labelColor": "#00382f"
          }
        },
        "tooltip": [
          {
            "field": "AEMO_Station_Name",
            "type": "nominal",
            "title": "Station Name"
          },
          {
            "field": "State Full Name",
            "type": "nominal",
            "title": "State"
          },
          {
            "field": "Fuel_Source",
            "type": "nominal",
            "title": "Fuel Source"
          },
          {
            "field": "Accreditation_Date_ISO",
            "type": "temporal",
            "title": "Accreditation Date",
            "format": "%d/%m/%Y"
          },
          {
            "field": "Installed_Capacity_MW",
            "type": "quantitative",
            "title": "Installed Capacity (MW)",
            "format": ".2f"
          },
          {
            "field": "Avg_MW",
            "type": "quantitative",
            "title": "Average Output (MW)",
            "format": ".2f"
          }
        ]
      }
    },
    {
      "data": { "values": [{}] },
